#include <iostream>
#include <cmath>
using namespace std;
int j = 0; float a; float d;
int main(int n) {
	cout << "Enter N - ";
	cin >> n;
	cout << "Enter A - ";
	cin >> a;
	cout << "Enter D - ";
	cin >> d;
	int ar[100] = {};
	for (int i = 0; i <= n - 1; i++) {
		ar[j] = a * pow(d, i);
		cout << ar[j] << endl;
		j++;
	}
}
